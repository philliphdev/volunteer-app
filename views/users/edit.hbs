<h1>Edit User</h1>

<form action="/users" method="POST">
  <div>
    <label for="name">User Name</label>
    <input type="text" name="name" value={{editUser.name}} />
  </div>
  <div>
    <label for="age">Age</label>
    <input type="number" name="age" value={{editUser.age}} />
  </div>
  <div>
    <label for="email">email address</label>
    <input type="email" name="email" value={{editUser.email}} />
  </div>
  <div>
    <label for="phone">Phone Number</label>
    <input type="tel" name="phone" value={{editUser.phone}} />
  </div>
  <div>
    <label for="contactMethod">Preferred contact method</label>
    <input type="text" name="contactMethod" value={{editUser.contactMethod}} />
  </div>
    <div>
    <label for="photo">Photo</label>
    <input type="text" name="photo" value={{editUser.photo}} />
  </div>
  <button type="submit">Save User</button>
</form>