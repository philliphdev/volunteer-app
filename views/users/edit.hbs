<div class="page">
<div class="pageForms">
<h1>Edit User</h1>
<form action="/users/{{editUsers._id}}?_method=PUT" method="POST">
  <div>
    <label for="name">User Name</label>
    <input type="text" name="name" value="{{editUsers.name}}" />
  </div>
  <div>
    <label for="age">Age</label>
    <input type="number" name="age" value="{{editUsers.age}}" />
  </div>
  <div>
    <label for="email">email address</label>
    <input type="email" name="email" value="{{editUsers.email}}"/>
  </div>
  <div>
    <label for="phone">Phone Number</label>
    <input type="tel" name="phone" value="{{editUsers.phone}}" />
  </div>
  <div>
    <label for="contactMethod">Preferred contact method</label>
    <input type="text" name="contactMethod" value="{{editUsers.contactMethod}}" />
  </div>
    <div>
    <label for="photo">Photo</label>
    <input type="text" name="photo" value="{{editUsers.photo}}" />
  </div>
  <button class="waves-effect waves-light red lighten-2 btn" type="submit">Save User</button>
</form>
</div>
</div>