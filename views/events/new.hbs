<h1>Create New User</h1>

<form action="/users" method="POST">
  <div>
    <label for="name">User Name</label>
    <input type="text" name="name" placeholder="Name: First Last" />
  </div>
  <div>
    <label for="age">Age</label>
    <input type="number" name="age" placeholder="Age" />
  </div>
  <div>
    <label for="email">email address</label>
    <input type="email" name="email" placeholder="email@domain.name" />
  </div>
  <div>
    <label for="phone">Phone Number</label>
    <input type="tel" name="phone" placeholder="1234567890" />
  </div>
  <div>
    <label for="contactMethod">Preferred contact method</label>
    <input type="text" name="contactMethod" placeholder="email or phone?" />
  </div>
    <div>
    <label for="photo">Photo</label>
    <input type="text" name="photo" placeholder="URL to photo" />
  </div>
  <button type="submit">Save User</button>
</form>